{
    "name": "filament/forms2",
    "description": "Effortlessly build TALL-powered forms.",
    "license": "MIT",
    "homepage": "https://github.com/laravel-filament/forms",
    "support": {
        "issues": "https://github.com/laravel-filament/forms/issues",
        "source": "https://github.com/laravel-filament/forms"
    },
    "require": {
        "php": "^8.0",
        "blade-ui-kit/blade-heroicons": "^1.2",
        "laravel/framework": "^8.5",
        "league/flysystem-aws-s3-v3": "^1.0",
        "livewire/livewire": "^2.5",
        "spatie/laravel-medialibrary": "^9.0",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "orchestra/testbench": "^6.19",
        "pestphp/pest": "^1.12",
        "phpstan/phpstan": "^0.12.94"
    },
    "autoload": {
        "psr-4": {
            "Filament\\Forms2\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "./vendor/bin/pest && vendor/bin/phpstan"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Filament\\Forms2\\FormsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
